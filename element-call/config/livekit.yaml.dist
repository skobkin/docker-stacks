port: 7880
bind_addresses:
  - "0.0.0.0"
rtc:
  # Keep in sync with LIVEKIT_RTC_TCP_PORT and UDP range in .env
  tcp_port: 7881
  port_range_start: 50000
  port_range_end: 50200
  use_external_ip: false
room:
  auto_create: false
logging:
  level: info
keys:
  # Must match LIVEKIT_API_KEY and LIVEKIT_API_SECRET in .env
  devkey: "change-me"
